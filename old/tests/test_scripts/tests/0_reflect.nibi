(assert (eq (type 24) "i64") "")
(assert (eq (type 3.14159) "f64") "")
(assert (eq (type "AYYYYLMAO") "string") "")
(assert (eq (type [1 2 3]) "list:data") "")
(assert (eq (type {}) "list:access") "")

(assert (eq (type (i8 0)) "i8"))
(assert (eq (type (i16 0)) "i16"))
(assert (eq (type (i32 0)) "i32"))
(assert (eq (type (i64 0)) "i64"))

(assert (eq (type (u8 0)) "u8"))
(assert (eq (type (u16 0)) "u16"))
(assert (eq (type (u32 0)) "u32"))
(assert (eq (type (u64 0)) "u64"))

(assert (eq (type (f32 0.0)) "f32"))
(assert (eq (type (f64 0.0)) "f64"))
